{
  "name": "atlas-docs",
  "version": "1.0.0",
  "type": "module",
  "description": "Atlas PHP Documentation",
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --fix",
    "lint:css": "stylelint \".vitepress/theme/**/*.css\" --fix",
