    "lint:css:check": "stylelint \".vitepress/theme/**/*.css\""
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@stylistic/stylelint-plugin": "^3.1.0",
    "eslint": "^9.18.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^37.0.0",
    "typescript-eslint": "^8.21.0",
    "vitepress": "^1.5.0",
    "vue": "^3.5.0"
  }
}
