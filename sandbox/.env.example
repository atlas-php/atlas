# Atlas Sandbox Environment Configuration

# OpenAI Provider
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_URL=https://api.openai.com/v1
OPENAI_ORGANIZATION=

# Anthropic Provider
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_API_VERSION=2023-06-01

# Gemini
GEMINI_API_KEY=

# Default Chat Settings
ATLAS_CHAT_PROVIDER=openai
ATLAS_CHAT_MODEL=gpt-4o

# Embedding Settings
ATLAS_EMBEDDING_PROVIDER=openai
ATLAS_EMBEDDING_MODEL=text-embedding-3-small
ATLAS_EMBEDDING_DIMENSIONS=1536
ATLAS_EMBEDDING_BATCH_SIZE=100

# Image Generation Settings
ATLAS_IMAGE_PROVIDER=openai
ATLAS_IMAGE_MODEL=dall-e-3

# Speech Settings
ATLAS_SPEECH_PROVIDER=openai
ATLAS_SPEECH_MODEL=tts-1
ATLAS_TRANSCRIPTION_MODEL=whisper-1
